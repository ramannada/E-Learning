            <button type="button" class="btn btn-sm btn-default disabled">Slim</button>
            <button type="button" class="btn btn-sm btn-default disabled">Redis</button>
            <br><br>
            <div>
              <div class="col-sm-4">
               <i class="fa fa-tasks item"> 16 parts</i>
               </div>
